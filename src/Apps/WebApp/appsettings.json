{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft": "Warning",
            "Microsoft.Hosting.Lifetime": "Information"
        }
    },
    "AllowedHosts": "*",
    "ConnectionStrings": {
        "DefaultConnection": "DataSource=app.db;Cache=Shared"
    },
    "BreachedEmails": {
        "ProbabilisticEmailAddressCollectionAdapter": {
            "MemoryAvailableInBytes": 67108864,
            "ProbabilisticThreshold": 0.01,
            "CollectionBehaviour": "Exact"
        },
        "InMemoryCacheEmailAddressCollectionAdapter": {
            "MemoryAvailableInBytes": 67108864
        }
    }
}
